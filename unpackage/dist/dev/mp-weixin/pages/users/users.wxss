
.index {
		display: flex;
		flex-direction: column;
		width: 100%;
		background-color: white;
		border-top: 1px solid #cccccc;
}
.vbox {
		display: flex;
		flex-direction: column;
		align-items: center;
}
.top_back_img {
		z-index: -1;
		position: absolute;
		top: 0rpx;
		width: 100%;
		height: 420rpx;
}
.top {
		display: flex;
		width: 100%;
		height: 420rpx;
		align-items: center;
}
.circle {
		margin-left: 100rpx;
		width: 120rpx;
		height: 120rpx;
		border: 10rpx solid #a4f4f6;
		border-radius: 150rpx;
		overflow: hidden;
}
.head {
		width: 120rpx;
		height: 120rpx;
		border-radius: 150rpx;
}
.top-texts {
		display: flex;
		flex-direction: column;
		margin-left: 15rpx;
		color: white;
}
.name {
		font-size: 36rpx;
		font-weight: 500;
}
.set-top-hr {
		width: 210rpx;
		height: 6rpx;
}
.top-changeInfo {
		margin-top: 250rpx;
		width: 120rpx;
		height: 28rpx;
		line-height: 28rpx;
		background-color: #FFFFFF;
		border-radius: 15rpx;
		padding: 10rpx;
		color: #33dce8;
}
.middle {
		display: flex;
		align-items: center;
		position: relative;
		top: -50rpx;
		width: 80%;
		height: 100rpx;
		background-color: white;
		border-radius: 15rpx;
}
.middle-line {
		width: 2rpx;
		height: 80rpx;
		background-color: #eeeeee;
}
.middle-left {
		flex-grow: 1;
		color: #666666;
		display: flex;
		align-items: center;
		text-align: center;
}
.middle-right {
		flex-grow: 1;
		color: #666666;
		display: flex;
		align-items: center;
		text-align: center;
}
.middle-icon {
		width: 40rpx;
		height: 40rpx;
		margin-left: 20rpx;
}
.middle-num {
		color: #fcac45;
		-webkit-font-smoothing: antialiased;
}
.cell {
		display: flex;
		align-items: center;
		border-bottom: 1px solid #ccc;
		height: 90rpx;
		align-items: center;
		justify-content: space-between;
}
.cell:active {
		background-color: #333;
		color: white;
		box-shadow: 1rpx 1rpx 35rpx #ccc;
}
.cell-left {
		display: flex;
		align-items: center;
		margin-left: 65rpx;
}
.cell-text {
		margin-left: 25rpx;
}
.cell-right {
		margin-right: 45rpx;
		height: 28rpx;
}
.cell_icon {
		width: 40rpx;
		height: 40rpx;
		height: 40rpx;
}
.right-arrow {
		color: #aaa;
		width: 15rpx;
		height: 28rpx;
}
